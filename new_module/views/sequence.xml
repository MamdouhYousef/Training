<odoo>
    <data>


        <!-- Payment sequences -->
        <record id="sequence_new_module" model="ir.sequence">
            <field name="name">New module sequence</field>
            <field name="code">new.module</field>
            <field name="prefix">NM/%(year)s-</field>
            <field name="suffix">#</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>